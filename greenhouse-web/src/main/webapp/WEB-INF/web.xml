<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_ID" version="2.4"
	xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
	<display-name>GreenHouse</display-name>
	<env-entry>
		<description>Интервал сканирования (минуты).</description>
		<env-entry-name>deltaspike/scannerInterval</env-entry-name>
		<env-entry-type>java.lang.String</env-entry-type>
		<env-entry-value>0 0/1 * 1/1 * ? *</env-entry-value>
	</env-entry>
	<env-entry>
		<description>Интервал получения данных (минуты).</description>
		<env-entry-name>deltaspike/getDataInterval</env-entry-name>
		<env-entry-type>java.lang.String</env-entry-type>
		<env-entry-value>0 0/1 * 1/1 * ? *</env-entry-value>
	</env-entry>
	<env-entry>
		<description>Автоматическое управление коньком.</description>
		<env-entry-name>deltaspike/auto</env-entry-name>
		<env-entry-type>java.lang.String</env-entry-type>
		<env-entry-value>true</env-entry-value>
	</env-entry>
	<env-entry>
		<description>Порт для связи с Arduino.</description>
		<env-entry-name>deltaspike/serialPort</env-entry-name>
		<env-entry-type>java.lang.String</env-entry-type>
		<env-entry-value>COM3</env-entry-value>
	</env-entry>
	<env-entry>
		<description>Максимальная температура.</description>
		<env-entry-name>deltaspike/tempMax</env-entry-name>
		<env-entry-type>java.lang.String</env-entry-type>
		<env-entry-value>32.0</env-entry-value>
	</env-entry>
	<env-entry>
		<description>Температура 2.</description>
		<env-entry-name>deltaspike/temp2</env-entry-name>
		<env-entry-type>java.lang.String</env-entry-type>
		<env-entry-value>28.0</env-entry-value>
	</env-entry>
	<env-entry>
		<description>Температура 1.</description>
		<env-entry-name>deltaspike/temp1</env-entry-name>
		<env-entry-type>java.lang.String</env-entry-type>
		<env-entry-value>25.0</env-entry-value>
	</env-entry>
	<env-entry>
		<description>Минимальная температура.</description>
		<env-entry-name>deltaspike/tempMin</env-entry-name>
		<env-entry-type>java.lang.String</env-entry-type>
		<env-entry-value>20.0</env-entry-value>
	</env-entry>
	<env-entry>
		<description>Максимальная влажность.</description>
		<env-entry-name>deltaspike/humMax</env-entry-name>
		<env-entry-type>java.lang.String</env-entry-type>
		<env-entry-value>70.0</env-entry-value>
	</env-entry>
